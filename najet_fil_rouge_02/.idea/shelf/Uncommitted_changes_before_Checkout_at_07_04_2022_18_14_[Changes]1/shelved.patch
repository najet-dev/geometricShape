+
+        ApiException apiException = new ApiException(
+                e.getMessage(),
+                e,
+                badRequest,
+                ZonedDateTime.now(ZoneId.of("Z"))
+        );
+        return new ResponseEntity<>(apiException, badRequest);
+
+    }
