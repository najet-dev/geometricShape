+        Shape shape = null;
+        switch (this.type) {
+            case CIRCLE:
+                shape = new Circle(this.id, this.x, this.y, this.radius);
+                break;
+            case RECTANGLE:
+                shape = new Rectangle(this.id, this.x, this.y, this.length, this.width);
+                break;
+            case TRIANGLE:
+                shape = new Triangle(this.id, this.x, this.y, this.side1, this.side2, this.side3);
+                break;
+            default:
+                break;
+        }
+        return shape;
+    }
